type ConfiguredFeatureRef = (
	#[id=worldgen/configured_feature] string,
	ConfiguredFeature,
)

struct ConfiguredFeature {
	type: #[id=worldgen/feature],
	config: minecraft:feature_config[[type]],
}

dispatch minecraft:feature_config[bamboo] to struct ProbabilityConfig {
	probability: float @ 0..1,
}

dispatch minecraft:feature_config[basalt_columns] to struct ColumnConfig {
	probability: float @ 0..1,
}
