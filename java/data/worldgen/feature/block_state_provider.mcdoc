struct BlockStateProvider {
	type: #[id=worldgen/block_state_provider_type] string,
	...minecraft:block_state_provider[[type]],
}

dispatch minecraft:block_state_provider[rotated_block_provider,simple_state_provider] to SimpleBlockStateProvider {
	state: BlockState,
}

dispatch minecraft:block_state_provider[weighted_state_provider] to WeightedBlockStateProvider {
	entries: [struct {
		weight: int @ 1..,
		data: BlockState,
	}]
}
