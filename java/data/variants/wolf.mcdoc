    biomes: (#[id(registry="worldgen/biome",tags=allowed)] string | [#[id="worldgen/biome"] string]),
	wild_texture: #[id(registry="texture", path="entity/wolf")] string,
    tame_texture: #[id(registry="texture", path="entity/wolf")] string,
    angry_texture: #[id(registry="texture", path="entity/wolf")] string,
}
