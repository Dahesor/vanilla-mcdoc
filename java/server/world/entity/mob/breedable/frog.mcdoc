use ::java::server::util::FlagMemory

#[since=1.19]
dispatch minecraft:entity[frog] to struct Frog {
	...super::Breedable,
	Brain: struct {
		memories: struct {
			[#[id] "is_pregnant"]: struct {
				value: struct {},
			},
			[#[id] "is_tempted"]: FlagMemory,
		},
	},
	#[until=1.20] // TODO: Idk when this actually got renamed
	Variant: #[id=frog_variant] string,
	#[since=1.20]
	variant: #[id=frog_variant] string,
}
