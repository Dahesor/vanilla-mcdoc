dispatch minecraft:entity[painting] to struct Painting {
	TileX: int,
	TileY: int,
	TileZ: int,
	/// Direction it is facing.
	Facing: Facing,
	/// Type of painting.
	#[until=1.19]
	Motive: #[id=motive] string,
	/// Type of painting.
	#[since=1.19]
	variant: #[id=painting_variant] string,
}

enum(byte) Facing {
	South = 0,
	West = 1,
	North = 2,
	East = 3,
}
