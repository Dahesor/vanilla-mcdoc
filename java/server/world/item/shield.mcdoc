use ::java::server::util::color::ColorInt
use ::java::server::util::BannerPattern

struct Shield {
	/// The tag for the shield
	BlockEntityTag: Tag,
}

struct Tag {
	/// The base color of the shield
	Base: ColorInt,
	/// The patterns on the shield
	Patterns: [Pattern],
}

struct Pattern {
	/// The color of the pattern
	Color: ColorInt,
	/// The pattern id
	Pattern: BannerPattern,
}
