dispatch minecraft:data_component[bees] to [Occupant]

struct Occupant {
	entity_data: minecraft:entity[[EntityTag.id]],
	min_ticks_in_hive: int,
	ticks_in_hive: int,
}

dispatch minecraft:item[bee_nest, beehive] to struct BeeContainer {
	[#[id] "bees"]?: minecraft:data_component[bees],
}
