use ::java::server::world::item::ItemStack

type SlottedItem<T> = struct {
	...ItemStack,
	/// Inventory slot the item is in
	Slot?: T,
}

enum(string) EquipmentSlot {
	Mainhand = "mainhand",
	Offhand = "offhand",
	Head = "head",
	Chest = "chest",
	Legs = "legs",
	Feet = "feet",
	#[since="1.20.5"]
	Body = "body",
}
